<openerp>
    <data>
        <template id="pos_ab_assets_backend"
                  name="pos_ab_assets_backend"
                  inherit_id="point_of_sale.assets_backend">
            <xpath expr="." position="inside">
                <script src="/ab_pos/static/src/js/models.js"
                        type="text/javascript"></script>
                <script src="/ab_pos/static/src/js/screens.js"
                        type="text/javascript"></script>
                <script src="/ab_pos/static/src/js/main.js"
                        type="text/javascript"></script>
                <script src="/ab_pos/static/src/js/widgets.js"
                        type="text/javascript"></script>
                <script src="/ab_pos/static/src/js/jquery.babypaunch.spinner.min.js"
                        type="text/javascript"></script>
            </xpath>
        </template>

        <template id="ab_pos_assets" name="ab_pos_css" inherit_id="point_of_sale.index">
            <xpath expr="//script[@id='loading-script']" position="before">
                <link rel="stylesheet" href="/ab_pos/static/src/css/pos.css"/>
            </xpath>
        </template>



    </data>
</openerp>
